{
    "plugins": [
        "cypress",
        "chai-friendly"
    ],
    "env": {
        "cypress/globals": true
    },
    "rules": {
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "overrides": [
      {
        "files": [
          "**/plugins/**/*.*"
        ],
        "env": {
          "node": true,
          "browser": false
        },
        "parserOptions": {
          "sourceType": "module"
        },
        "rules": {
          "no-console": "off"
        }
      }
    ]
}